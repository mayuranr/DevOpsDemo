pipeline {
    agent any

    stages {
        stage('compile stage') {
            steps {
                withMaven(maven: 'maven_3_6_3') {
                sh 'mvn clean compile'
                }
            }
        }
        stage('build') {
            steps {
                withMaven(maven: 'maven_3_6_3') {
                sh 'mvn build'
            }
        }
        stage('Test') {
            steps {
                withMaven(maven: 'maven_3_6_3') {
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                withMaven(maven: 'maven_3_6_3') {
                sh 'mvn deploy'
            }
        }
    }
}
}
}
}
